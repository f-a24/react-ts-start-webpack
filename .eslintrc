{
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "plugins": ["react", "jsx-a11y", "import", "@typescript-eslint"],
    "env": { "browser": true },
    "rules": {
      "comma-dangle": 0,
      "arrow-parens": 0,
      "no-duplicate-imports": 0,
      "no-shadow": 0
    }
  }